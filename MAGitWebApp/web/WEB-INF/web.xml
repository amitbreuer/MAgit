<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlets.loginPageServlet.LoginServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>HeadBranchInformationServlet</servlet-name>
        <servlet-class>servlets.repositoryInformationPageServlets.HeadBranchInformationServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>MainFolderOfCommitServlet</servlet-name>
        <servlet-class>servlets.repositoryInformationPageServlets.MainFolderOfCommitServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RepositoryNameAndRRDataServlet</servlet-name>
        <servlet-class>servlets.repositoryInformationPageServlets.RepositoryNameAndRRDataServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CurrentUserInformationServlet</servlet-name>
        <servlet-class>servlets.userInformationPageServlets.CurrentUserInformationServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ForkServlet</servlet-name>
        <servlet-class>servlets.userInformationPageServlets.ForkServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>servlets.userInformationPageServlets.LogoutServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>NewRepositoryServlet</servlet-name>
        <servlet-class>servlets.userInformationPageServlets.NewRepositoryServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>OtherUsersInformationServlet</servlet-name>
        <servlet-class>servlets.userInformationPageServlets.OtherUsersInformationServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>WatchRepositoryServlet</servlet-name>
        <servlet-class>servlets.userInformationPageServlets.WatchRepositoryServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>WCStatusServlet</servlet-name>
        <servlet-class>servlets.repositoryInformationPageServlets.WCStatusServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>OtherBranchesInformationServlet</servlet-name>
        <servlet-class>servlets.repositoryInformationPageServlets.OtherBranchesInformationServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DeleteBranchServlet</servlet-name>
        <servlet-class>servlets.repositoryInformationPageServlets.DeleteBranchServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CheckoutBranchServlet</servlet-name>
        <servlet-class>servlets.repositoryInformationPageServlets.CheckoutBranchServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CreateNewBranchServlet</servlet-name>
        <servlet-class>servlets.repositoryInformationPageServlets.CreateNewBranchServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PushServlet</servlet-name>
        <servlet-class>servlets.repositoryInformationPageServlets.PushServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>PullServlet</servlet-name>
        <servlet-class>servlets.repositoryInformationPageServlets.PullServlet</servlet-class>
    </servlet>

<servlet>
        <servlet-name>MessagesServlet</servlet-name>
        <servlet-class>servlets.userInformationPageServlets.MessagesServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/pages/login/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>HeadBranchInformationServlet</servlet-name>
        <url-pattern>/headBranchInformation</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>MainFolderOfCommitServlet</servlet-name>
        <url-pattern>/mainFolderOfCommit</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RepositoryNameAndRRDataServlet</servlet-name>
        <url-pattern>/repositoryNameAndRRData</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>WCStatusServlet</servlet-name>
        <url-pattern>/wcStatus</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CurrentUserInformationServlet</servlet-name>
        <url-pattern>/currentUserInformation</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ForkServlet</servlet-name>
        <url-pattern>/fork</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>NewRepositoryServlet</servlet-name>
        <url-pattern>/newRepository</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>OtherUsersInformationServlet</servlet-name>
        <url-pattern>/otherUsersInformation</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>WatchRepositoryServlet</servlet-name>
        <url-pattern>/watchRepository</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>OtherBranchesInformationServlet</servlet-name>
        <url-pattern>/otherBranchesInformation</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DeleteBranchServlet</servlet-name>
        <url-pattern>/deleteBranch</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CheckoutBranchServlet</servlet-name>
        <url-pattern>/checkout</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CreateNewBranchServlet</servlet-name>
        <url-pattern>/createNewBranch</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PushServlet</servlet-name>
        <url-pattern>/push</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>PullServlet</servlet-name>
        <url-pattern>/pull</url-pattern>
    </servlet-mapping>

 <servlet-mapping>
        <servlet-name>MessagesServlet</servlet-name>
        <url-pattern>/messages</url-pattern>
    </servlet-mapping>

</web-app>